CREATE TABLE GOVERNING_STATE (
    GOVERNING_STATE_ID NUMBER,
    ZIP_CODE varchar(255),
    GOVERNING_STATE varchar(255),
    CREATED_ON DATE
);

create sequence GOVERNING_STATE_SEQUENCE start with 1000;

